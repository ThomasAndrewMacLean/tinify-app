{
    "name": "electron-quick-start",
    "version": "1.0.0",
    "description": "A minimal Electron application",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "build": "electron-packager ./ tinify --platform darwin --out ./dist",
        "postbuild": "cp -R ./dist/tinify-darwin-x64/tinify.app /Applications"
    },
    "repository": "https://github.com/electron/electron-quick-start",
    "keywords": [
        "Electron",
        "quick",
        "start",
        "tutorial",
        "demo"
    ],
    "author": "GitHub",
    "license": "CC0-1.0",
    "devDependencies": {
        "electron": "^18.3.7",
        "electron-packager": "^15.3.0"
    },
    "dependencies": {
        "electron-reload": "^1.5.0",
        "tinify": "^1.6.0-beta.2"
    }
}
